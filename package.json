{
    "name": "ten-thousand",
    "version": "1.0.6",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "start:ssr": "npm run build:demo:ssr && npm run serve:ssr",
        "start:prerender": "npm run build:demo:prerender && npm run serve:prerender",
        "build": "ng build",
        "build:schematics": "tsc -p tsconfig.schematics.json",
        "build:demo:client": "ng run demo:build",
        "build:demo:server": "ng run demo:server",
        "build:demo:ssr": "npm run build:demo:client && npm run build:demo:server && npm run compile:server",
        "build:demo:prerender": "npm run build:demo:ssr && npm run generate:prerender",
        "serve:ssr": "node dist/demo/ssr/server",
        "serve:prerender": "cd dist/demo/browser && http-server",
        "compile:server": "tsc -p ./projects/demo/tsconfig.ssr.json",
        "generate:prerender": "node dist/demo/ssr/prerender",
        "test": "ng test",
        "posttest": "node scripts/coveralls.js",
        "preadd": "npm run build:schematics",
        "add": "schematics ./schematics/collection.json:library-starter --debug=false",
        "postadd": "git add ./projects",
        "lint": "ng lint",
        "typecheck": "tsc --noEmit --skipLibCheck",
        "release": "standard-version",
        "release:patch": "npm run release -- --release-as patch",
        "release:minor": "npm run release -- --release-as minor",
        "release:major": "npm run release -- --release-as major",
        "publish": "npm run build:all && npm publish:all",
        "build:all": "npm run build:ten-thousand",
        "publish:all": "npm run publish:ten-thousand",
        "build:ten-thousand": "ng run ten-thousand:build",
        "test:ten-thousand": "ng run ten-thousand:test",
        "test:ten-thousand:watch": "ng run ten-thousand:test --watch=true",
        "publish:ten-thousand": "npm publish ./dist/ten-thousand"
    },
    "description": "This is a template project to develop, create and open-source Angular libraries with demos",
    "keywords": ["angular", "ng", "workspace", "library", "starter", "tinkoff"],
    "license": "Apache-2.0",
    "authors": ["Roman Sedov <r.sedov@tinkoff.ru>", "Alex Inkin <a.inkin@tinkoff.ru>"],
    "repository": "https://github.com/TinkoffCreditSystems/angular-library-starter",
    "bugs": "https://github.com/TinkoffCreditSystems/angular-library-starter/issues",
    "homepage": "https://github.com/TinkoffCreditSystems/angular-library-starter#README",
    "schematics": "./schematics/collection.json",
    "dependencies": {
        "@angular/animations": "^9.0.7",
        "@angular/common": "^9.0.7",
        "@angular/compiler": "^9.0.7",
        "@angular/core": "^9.0.7",
        "@angular/forms": "^9.0.7",
        "@angular/platform-browser": "^9.0.7",
        "@angular/platform-browser-dynamic": "^9.0.7",
        "@angular/platform-server": "^9.0.7",
        "@angular/router": "^9.0.7",
        "@fortawesome/angular-fontawesome": "^0.6.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.27",
        "@fortawesome/free-brands-svg-icons": "^5.12.1",
        "@fortawesome/free-regular-svg-icons": "^5.12.1",
        "@fortawesome/free-solid-svg-icons": "^5.12.1",
        "@nguniversal/common": "^9.0.2",
        "@nguniversal/express-engine": "^9.0.2",
        "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
        "core-js": "^2.5.4",
        "rxjs": "^6.5.4",
        "tslib": "^1.10.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.900.7",
        "@angular-devkit/build-ng-packagr": "~0.900.7",
        "@angular-devkit/core": "^9.0.7",
        "@angular-devkit/schematics": "^9.0.7",
        "@angular-devkit/schematics-cli": "~0.13.9",
        "@angular/cli": "^9.0.7",
        "@angular/compiler-cli": "^9.0.7",
        "@angular/language-service": "^9.0.7",
        "@tinkoff/linters": "^0.4.0",
        "@types/express": "4.16.1",
        "@types/express-serve-static-core": "4.16.2",
        "@types/jasmine": "^3.3.16",
        "@types/jasminewd2": "^2.0.6",
        "@types/node": "^12.11.1",
        "codelyzer": "^5.1.2",
        "coveralls": "^3.0.5",
        "ecstatic": "^4.1.2",
        "express": "^4.17.1",
        "http-server": "^0.11.1",
        "husky": "^3.0.2",
        "jasmine-core": "~3.4.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^4.2.0",
        "karma-chrome-launcher": "^3.0.0",
        "karma-coverage-istanbul-reporter": "^2.1.0",
        "karma-jasmine": "~2.0.1",
        "karma-jasmine-html-reporter": "^1.4.2",
        "lint-staged": "^9.2.1",
        "lodash": "^4.17.15",
        "ng-packagr": "^9.0.0",
        "prettier": "^1.18.2",
        "standard-version": "^7.0.0",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "tsutils": "^3.17.1",
        "typescript": "~3.7.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged && npm run typecheck"
        }
    },
    "lint-staged": {
        "*.{js,ts,html,md,less,json}": ["prettier --write", "git add"],
        "*.ts": "tslint"
    },
    "standard-version": {
        "scripts": {
            "postbump": "node scripts/syncVersions.js && git add **/package.json"
        }
    },
    "engines": {
        "node": ">= 10",
        "npm": ">= 3"
    }
}
