{
  "name": "@coderbase/ten-thousand",
  "version": "1.1.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "npm run build:demo:ssr && npm run serve:ssr",
    "start:prerender": "npm run build:demo:prerender && npm run serve:prerender",
    "build": "ng build",
    "build:schematics": "tsc -p tsconfig.schematics.json",
    "build:demo:client": "ng run demo:build",
    "build:demo:server": "ng run demo:server",
    "build:demo:ssr": "npm run build:demo:client && npm run build:demo:server && npm run compile:server",
    "build:demo:prerender": "npm run build:demo:ssr && npm run generate:prerender",
    "serve:ssr": "node dist/demo/ssr/server",
    "serve:prerender": "cd dist/demo/browser && http-server",
    "compile:server": "tsc -p ./projects/demo/tsconfig.ssr.json",
    "generate:prerender": "node dist/demo/ssr/prerender",
    "test": "ng test",
    "posttest": "node scripts/coveralls.js",
    "preadd": "npm run build:schematics",
    "add": "schematics ./schematics/collection.json:library-starter --debug=false",
    "postadd": "git add ./projects",
    "lint": "ng lint",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "release": "standard-version",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "publish": "npm run build:all && npm publish:all",
    "build:all": "npm run build:ten-thousand && npm run build:angular-yeti-login && npm run build:ngx-breadcrumbs",
    "publish:all": "npm run publish:ten-thousand && npm run publish:angular-yeti-login && npm run publish:ngx-breadcrumbs",
    "build:ten-thousand": "ng run ten-thousand:build",
    "test:ten-thousand": "ng run ten-thousand:test",
    "test:ten-thousand:watch": "ng run ten-thousand:test --watch=true",
    "publish:ten-thousand": "npm publish ./dist/ten-thousand --access public",
    "build:ngx-breadcrumbs": "ng run ngx-breadcrumbs:build",
    "test:ngx-breadcrumbs": "ng run ngx-breadcrumbs:test",
    "test:ngx-breadcrumbs:watch": "ng run ngx-breadcrumbs:test --watch=true",
    "publish:ngx-breadcrumbs": "npm publish ./dist/ngx-breadcrumbs --access public"
  },
  "description": "This is a template project to develop, create and open-source Angular libraries with demos",
  "keywords": [
    "angular",
    "ng",
    "workspace",
    "library",
    "starter",
    "coderbase.io"
  ],
  "license": "Apache-2.0",
  "authors": [
    "Pierre Nédélec <p.nedelec@coderbase.io>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com:coderbase-it/angular-ten-thousand.git"
  },
  "bugs": {
    "url": "https://github.com:coderbase-it/angular-ten-thousand.git/issues"
  },
  "homepage": "https://github.com:coderbase-it/angular-ten-thousand#README",
  "schematics": "./schematics/collection.json",
  "private": false,
  "dependencies": {
    "@angular/animations": "^9.1.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@angular/platform-server": "^9.1.0",
    "@angular/router": "^9.1.0",
    "@fortawesome/angular-fontawesome": "^0.6.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@ng-bootstrap/ng-bootstrap": "^6.0.2",
    "@nguniversal/common": "^9.1.0",
    "@nguniversal/express-engine": "^9.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "application": "^0.1.4",
    "core-js": "^3.6.4",
    "rxjs": "^6.5.5",
    "tsickle": "^0.38.1",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular-devkit/core": "^9.1.0",
    "@angular-devkit/schematics": "^9.1.0",
    "@angular-devkit/schematics-cli": "~0.901.0",
    "@angular/cli": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/language-service": "^9.1.0",
    "@tinkoff/linters": "^1.0.0-rc.5",
    "@types/express": "4.17.4",
    "@types/express-serve-static-core": "4.17.3",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.11.0",
    "codelyzer": "^5.2.2",
    "coveralls": "^3.0.11",
    "ecstatic": "^4.1.4",
    "express": "^4.17.1",
    "http-server": "^0.12.1",
    "husky": "^4.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "lint-staged": "^10.1.2",
    "lodash": "^4.17.15",
    "ng-packagr": "^9.1.0",
    "prettier": "^2.0.4",
    "standard-version": "^7.1.0",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "tsutils": "^3.17.1",
    "typescript": "~3.8.3"
  },
  "lint-staged": {
    "*.{js,ts,html,md,less,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": "tslint"
  },
  "standard-version": {
    "scripts": {
      "postbump": "node scripts/syncVersions.js && git add **/package.json"
    }
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 3"
  },
  "main": "prettier.config.js",
  "author": "coderbaseit@gmail.com"
}
